api:
  - get
  - set
--- |
<%- $.outfile('client/client.js') _%>

<%_ for (let name of api) { _%>
function <%- name %>() {
  return fetch('/<%- name %>');
}

<%_ } _%>

<%- $.outfile('server/server.js') _%>

function register(app) {
<%_ for (let name of api) { _%>
  app.get('/<%- name %>', (req, res) => {
<%- $.block(name.toUpperCase() + '_IMPL', `
    const result = { api: '${name}' };
    res.send(result);
`) %>
  });

<%_ } _%>
}

